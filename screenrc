startup_message off
defscrollback 5000
screen -t (a)
screen -t (b)
screen -t (c)
screen -t (d)
screen -t (e)
screen -t (f)
screen -t (g)
hardstatus alwayslastline
hardstatus string '%{= kG}%-Lw%{= kW}%50> %n*%f %t%{= kG}%+Lw%< %{= kG}%-=%D %m/%d/%y | %C:%s %A | %1`%{-}'
bindkey "^k" prev
bindkey "^j" next
fit
